{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "pet"
      ],
      "properties": {
        "pet": {
          "type": "object",
          "required": [
            "name"
          ],
          "properties": {
            "name": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "pets"
      ],
      "properties": {
        "pets": {
          "type": "object",
          "required": [
            "page_num",
            "page_size"
          ],
          "properties": {
            "page_num": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "page_size": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      }
    }
  ]
}
